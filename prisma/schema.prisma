generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id           Int      @id @default(autoincrement())
  name         String
  email        String   @unique
  hashedPassword String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
model Apertment {
  id           Int      @id @default(autoincrement())
  name         String
  email        String   @unique
  hashedPassword String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}